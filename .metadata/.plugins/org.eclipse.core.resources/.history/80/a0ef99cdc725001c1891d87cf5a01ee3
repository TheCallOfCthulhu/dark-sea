import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;
import java.util.Random;


public class training {
	
	
	public static void main(String[] args) {
		
		int[] pole = {25, 14, 56, 15, 36, 56, 77, 18, 29, 49};
		   System.out.println(pole);
		   int[] pole2 = new int [pole.length];
	    // Insert an element in 3rd position of the array (index->2, value->5)
	   
	   int Index_position = 2;
	   int newValue    = 5;

	  System.out.println("Original Array : "+Arrays.toString(pole));     
	   
	  for(int i=0; i <pole.length-1; i++){
	    pole2[pole2.length-1-i] = pole[i];
	   }
	  
	   System.out.println("New Array: "+Arrays.toString(pole2));
	   
	   System.out.println(pole);
	 
	}
}

