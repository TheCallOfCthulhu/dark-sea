import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;
import java.util.Random;


public class training {
	
	
	public static void main(String[] args) {
		
		int[] pole = new int [] {25, 14, 56, 15, 36, 56, 77, 18, 29, 49};
		   System.out.println(pole);
		  ArrayList<Integer> pole2 = new ArrayList<Integer>(Arrays.asList(pole));
	    // Insert an element in 3rd position of the array (index->2, value->5)
	   
		  String[]  my_array = new String[] {"Python", "JAVA", "PHP",  "Perl", "C#", "C++"};
		  ArrayList<String>  list = new ArrayList<String>(Arrays.asList(my_array));

	  System.out.println("Original Array : "+Arrays.toString(pole));     
	   
	  for(int policko:pole){
	    pole2.add(policko);
	   }
	  
	   System.out.println(pole2);
	   
	   System.out.println(pole);
	 
	}
}

